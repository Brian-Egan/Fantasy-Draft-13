%tr{:class => 'playerRow', :'data-name' => p.name}
	%td{:class => 'info tBox'} #{p.rank1}
	%td{:class => 'info tBox  '}
		= (p.rank2/16).ceil
	%td{:class => 'info '} #{p.name}
	%td{:class => 'info prBox '} #{p.position} - #{p.pos_rank}
	%td{:class => 'info prBox centerText seasonPoints'} #{p.rank3}
	%td{:class => 'info wklyPoints centerText'} #{p.wkly_points.ceil}
	%td{:class => 'info info-btn'}
		-# - if p.rank1/12 < adr
		- if p.value == "Under"
			%button{:class => "btn btn-xs btn-info vBtn"} Value
		- elsif p.value == "Right"
			%button{:class => "btn btn-xs btn-success vBtn"} Right
		- elsif p.value == "Over"
			%button{:class => "btn btn-xs btn-warning vBtn"} Over
		- elsif p.value == "Over x2"
			%button{:class => "btn btn-xs btn-danger vBtn"} Over+
		- elsif p.value == "Extreme Over"
			%button{:class => "btn btn-xs btn-danger vBtn"} Extreme