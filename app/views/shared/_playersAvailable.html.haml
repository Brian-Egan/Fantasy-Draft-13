%div{:class => 'col-md-4'}
	%table{:class => 'table table-striped table-condensed col-md-3'}
		%tr
			%th{:class => 'tBox'} Ovrl.
			%th{:class => 'tBox'} ADR
			%th Name
			%th{:class => 'tBox'} Pos.
			%th
		- @players1.each do |p|
			- if (@roundNum - p.rank1) > 1
				= render :partial => 'shared/playerRowHighlight', :locals => {:p => p }
			- elsif p.rank1 <= @roundNum
				= render :partial => 'shared/playerRowSuccess', :locals => {:p => p }
			- else
				= render :partial => 'shared/playerRow', :locals => {:p => p }
